package com.kadirayk.ireadnothing.fragments;

import com.kadirayk.ireadnothing.R;

import android.os.Bundle;
import android.support.v4.app.Fragment;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;

public class YMLEFragment extends Fragment{

	private View mView;

	@Override
	public View onCreateView(LayoutInflater inflater, ViewGroup container,
			Bundle savedInstanceState) {
		
		mView = inflater.inflate(R.layout.fragment_ymle, container, false);
		
		return mView;
	}
	
	private void setUI(){
		
		fragment_debe_lv = (ListView) mView.findViewById(R.id.fragment_debe_listview);
		fragment_debe_lv.setOnItemClickListener(this);
		
	}
	
}
